{
  "compilerOptions": {
    "outDir": "./dist/",
    "baseUrl": ".",
    "paths": {
      "@pages/*": ["app/pages/*"],
      "@components/*": ["app/components/*"],
      "@contexts/*": ["app/contexts/*"],
      "@styles/*": ["app/styles/*"],
    },
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "esModuleInterop": true
  }
}
